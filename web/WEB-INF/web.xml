<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>FetchFoodServlet</servlet-name>
        <servlet-class>servlet.FetchFoodServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FetchFoodServlet</servlet-name>
        <url-pattern>/FetchFoodServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>FetchDiscountServlet</servlet-name>
        <servlet-class>servlet.FetchDiscountServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FetchDiscountServlet</servlet-name>
        <url-pattern>/FetchDiscountServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>FetchOrderServlet</servlet-name>
        <servlet-class>servlet.FetchOrderServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FetchOrderServlet</servlet-name>
        <url-pattern>/FetchOrderServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>FetchShopServlet</servlet-name>
        <servlet-class>servlet.FetchShopServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FetchShopServlet</servlet-name>
        <url-pattern>/FetchShopServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>FetchShopListServlet</servlet-name>
        <servlet-class>servlet.FetchShopListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FetchShopListServlet</servlet-name>
        <url-pattern>/FetchShopListServlet</url-pattern>
    </servlet-mapping>
</web-app>